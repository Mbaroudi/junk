@# /etc/conf.d/local.start
# $Header: /var/cvsroot/gentoo-src/rc-scripts/etc/conf.d/local.start,v 1.4.4.1 2005/02/19 02:13:53 vapier Exp $

# This is a good place to load any misc.
# programs on startup ( 1>&2 )
/etc/init.d/LBcore.init start
/etc/init.d/LBarcd.init start
/etc/init.d/LBcd.init.tables start

# PPPOE Server
/usr/sbin/pppoe-server -I eth1.12 -S pppoe -C netcom-r
/usr/sbin/pppoe-server -I eth1.13 -S pppoe -C netcom-r
/usr/sbin/pppoe-server -I eth1.15 -S pppoe -C netcom-r
/usr/sbin/pppoe-server -I eth1.24 -S pppoe -C netcom-r
/usr/sbin/pppoe-server -I eth1.27 -S pppoe -C netcom-r
/usr/sbin/pppoe-server -I eth1.29 -S pppoe -C netcom-r

# Iproute2
# for dns (ns2.rubin.ru)
/sbin/ip rule add from 195.210.145.4/32 to 0.0.0.0/0 table dns
/sbin/ip route add default table dns via 195.210.145.1
# for go to LB via direct connect. Not via proxy
/sbin/ip rule add from 212.248.100.3/32 to 0.0.0.0/0 table rubin_real
/sbin/ip route add default table rubin_real via 212.248.100.1
@
