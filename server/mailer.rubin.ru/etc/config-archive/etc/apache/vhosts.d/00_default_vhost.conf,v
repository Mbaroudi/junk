<VirtualHost *:80>

	DocumentRoot "/var/www/localhost/htdocs"
	
	<Directory "/var/www/localhost/htdocs">
		Options Indexes FollowSymLinks MultiViews
		AllowOverride None
		Order allow,deny
		Allow from all
	</Directory>

